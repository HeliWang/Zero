<configuration>
  <connectionStrings>
    <!--<add name="ZeroData" connectionString="server=127.0.0.1;database=Zero;uid=zero;pwd=zero" providerName="System.Data.SqlClient"/>-->
    <add name="ZeroData" connectionString="Server=localhost;Initial Catalog=Zero;Integrated Security=SSPI" providerName="System.Data.SqlClient"/>
  </connectionStrings>
    <system.web>
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />      
    </system.web>
  <system.webServer>
    <httpErrors errorMode="Custom">
      <remove statusCode="404" subStatusCode="-1" />
      <error statusCode="404" prefixLanguageFilePath="" path="/FileNotFound.aspx" responseMode="ExecuteURL" />
      <remove statusCode="403" subStatusCode="-1" />
      <error statusCode="403" prefixLanguageFilePath="" path="/FileNotFound.aspx" responseMode="ExecuteURL" />
    </httpErrors>
  </system.webServer>
</configuration>
